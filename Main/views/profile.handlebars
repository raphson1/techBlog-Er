
<p>test</p>
  {{#each blogs as |blog| }}
<div class="blog">

    <div class="blogTitle" id="blogTitle">
        <h3>
            <a href="/blog/{{blog.id}}">{{blog.title}}</a>
        </h3>
        <p>Posted by {{blog.user.name}} on {{ blog.date_created}}</p>
    </div>
    <div class="description">
        <p>
            {{blog.description}}
        </p>
    </div>
</div>
{{/each}}

<div class="btnBlog">
    <button type="submit" id="btnBlog">Create a new Blog?</button>
</div>
<script src="./js/profile.js"></script>
