
<div>
    {{#if blogs.length}}
    <div>
        <h3>Blogs</h3>
        {{#each blogs as |blog|}}
        <div class="blogTitle" id="blogTitle">
            <h3> <a href="/blog/{{id}}">{{title}}</a> </h3>
            <p>Posted by {{user.name}} on {{date_created}}</p>
        </div>
        <div class="description">
            <p> {{description}} </p>
        </div>
    </div>
    {{/each}}
    {{/if}}
</div>

<script src="./js/profile.js"></script>
